{
	"useTabs": true,
	"tabWidth": 4,
	"semi": true,
	"singleQuote": true,
	"trailingComma": "none",
	"overrides": [
		{
			"files": "*.md",
			"options": {
				"proseWrap": "always",
				"printWidth": 120
			}
		},
		{
			"files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
			"options": {
				"printWidth": 9999,
				"proseWrap": "never"
			}
		}
	]
}
